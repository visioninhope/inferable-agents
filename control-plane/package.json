{
  "name": "@inferable/control-plane",
  "version": "0.0.0",
  "private": true,
  "description": "Inferable control plane is the backend for the Inferable platform.",
  "scripts": {
    "test": "tsx -r dotenv/config src/utilities/run-migrate.ts && jest src --setupFiles dotenv/config --forceExit --testPathIgnorePatterns=.*\\.ai\\.test\\.ts$ --testMatch='**/*.test.ts'",
    "test:ai:eval": "tsx -r dotenv/config src/modules/workflows/agent/evaluation",
    "test:ai": "jest src --testMatch='**/*.ai.test.ts' --setupFiles dotenv/config --forceExit",
    "test:dev": "jest --watch  src --setupFiles dotenv/config --forceExit --onlyChanged --testMatch='**/*.test.ts'",
    "dev": "tsx --watch -r dotenv/config src/index.ts",
    "migrations": "npx drizzle-kit generate",
    "migrate": "tsx -r dotenv/config src/utilities/run-migrate.ts",
    "studio": "npx drizzle-kit studio",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.json --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check $(git diff --diff-filter=d --name-only origin/main HEAD | grep -E \"\\.(ts|tsx)$\" || echo \".\") || exit 0"
  },
  "author": "Inferable, Inc.",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.10.0",
    "@anthropic-ai/sdk": "^0.30.1",
    "@aws-sdk/client-s3": "^3.685.0",
    "@aws-sdk/client-sqs": "^3.637.0",
    "@clerk/backend": "^1.21.4",
    "@fastify/cors": "^8.5.0",
    "@hyperdx/node-opentelemetry": "^0.8.1",
    "@langchain/cohere": "^0.3.1",
    "@langchain/langgraph": "^0.1.9",
    "@nangohq/node": "^0.48.1",
    "@slack/bolt": "^4.1.1",
    "@toolhouseai/sdk": "^1.0.4",
    "@ts-rest/core": "^3.27.0",
    "@ts-rest/fastify": "3.45.2",
    "@ts-rest/open-api": "^3.45.2",
    "advisory-lock": "^2.0.0",
    "ajv": "=8.17.1",
    "ajv-formats": "=3.0.1",
    "async-retry": "^1.3.3",
    "dereference-json-schema": "^0.2.1",
    "drizzle-kit": "^0.22.0",
    "drizzle-orm": "^0.31.0",
    "fastify": "^4.21.0",
    "fastify-plugin": "^4.5.1",
    "flagsmith-nodejs": "^3.3.3",
    "inferable": "^0.30.59",
    "jest": "^29.6.4",
    "js-tiktoken": "^1.0.12",
    "jsonpath": "^1.1.1",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "langfuse": "^3.31.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "pem-jwk": "^2.0.0",
    "pg": "^8.11.2",
    "posthog-node": "^4.2.0",
    "redis": "^4.7.0",
    "rollbar": "^2.26.4",
    "skmeans": "^0.11.3",
    "sqs-consumer": "^11.1.0",
    "striptags": "^3.2.0",
    "typescript": "^5.1.6",
    "ulid": "^2.3.0",
    "winston": "^3.12.0",
    "winston-transport": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@nangohq/types": "^0.48.1",
    "@types/async-retry": "^1.4.9",
    "@types/jest": "^29.5.14",
    "@types/jsonpath": "^0.2.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.17.6",
    "@types/pg": "^8.10.2",
    "@types/skmeans": "^0.11.7",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "dotenv": "^16.3.1",
    "msw": "^2.7.0",
    "prettier": "^3.2.5",
    "prettier-2": "npm:prettier@^2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0"
  }
}